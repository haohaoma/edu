.slide-image{
  width: 100%;
  hegiht: auto;
}
.spacer{
  flex: 1;
}
.nav-bar{
  background: #fff;
  margin: 10px 0;
  padding: 5px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-wrap: wrap;
}
.nav-item{
  width: 25%;
  text-align: center;
  color: #000;
  padding: 5px;
  box-sizing: border-box;
}
.nav-text{
  display: block;
  padding: 10px 0;
  font-size: 12px;
}
.navigator-hover{
  color: #09bb07;
  background: transparent;
}
.icon-color{
  color: #09bb07 !important;
}

.map-botton{
  margin: 0 10px;
}
.secend{
  padding-left: 15px;
  padding-right: 15px;
}
.text{
  max-width: 80%;
  word-break: break-all;
  font-size: 14px;
  box-sizing: border-box;
  padding-right: 10px;
}